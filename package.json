{
  "name": "fluture-node",
  "version": "2.1.0",
  "description": "FP-style HTTP and streaming utils for Node based on Fluture",
  "keywords": [
    "buffer",
    "events",
    "fluture",
    "http",
    "https",
    "node",
    "request",
    "streams",
    "timers"
  ],
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./index.js": "./index.js"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "codecov": "codecov",
    "doctest": "sanctuary-doctest",
    "lint": "sanctuary-lint",
    "release": "sanctuary-release",
    "test": "npm run lint && sanctuary-test && npm run doctest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fluture-js/fluture-node.git"
  },
  "files": [
    "/index.cjs",
    "/index.js",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "author": "Aldwin Vlasblom <aldwin@avaq.it> (https://github.com/Avaq)",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "fluture": ">=12.0.0 <13.0.0"
  },
  "devDependencies": {
    "c8": "^7.1.0",
    "codecov": "^3.2.0",
    "fluture": "^12.2.0",
    "oletus": "^3.0.0",
    "rollup": "^2.0.0",
    "sanctuary-scripts": "^4.0.0"
  }
}
